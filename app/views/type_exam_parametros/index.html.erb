<% content_for :content do %>
  <p style="color: green"><%= notice %></p>

  <h1>Tipo examen parametros</h1>

  <%= link_to "Agregar tipo examen parametro", new_type_exam_parametro_path, :class => "btn btn-outline-success" %>

  <div id="typeexamparametro">
    <div class="card-body">
      <table id="datatablesSimple" class="table">
        <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Parametro:</th>
          <th scope="col">Tipo Examen:</th>
          <th scope="col">Acciones</th>
        </tr>
        </thead>
        <tbody>
        <div hidden="true"><%= @number = 1 %></div>
        <% @type_exam_parametros.each do |type_exam_parametro| %>
          <tr>
            <th scope="row"><%= @number %></th>
            <td><%= type_exam_parametro.parametro.nombre_parametro %></td>      
            <td><%= type_exam_parametro.type_exam.name_type_examn %></td>
            <td><%= link_to "Ver", type_exam_parametro, :class => "btn btn-outline-primary" %></td>
            <div hidden="true"><%= @number = @number + 1 %></div>
          </tr>
          <%#= render country %>
          <!--              <p><%#= link_to "Ver", muestra, :class=>"btn btn-outline-primary" %></p>-->
        <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <%#= link_to "Agregar Pais", new_country_path %>


<% end %>
<%= render template: "layouts/base" %>
