  <p>
    <strong>Apellido casado:</strong>
    <%= paciente.apellido_casado %>
  </p>

  <p>
    <strong>Estado civil:</strong>
    <%= paciente.estado_civil %>
  </p>

  <p>
    <strong>Dui paciente:</strong>
    <%= paciente.dui_paciente %>
  </p>

  <p>
    <strong>Pasaporte:</strong>
    <%= paciente.pasaporte %>
  </p>

  <p>
    <strong>Municipio:</strong>
    <%= paciente.municipio.nombre_municipio %>
  </p>

  <p>
    <strong>Tipo sangre:</strong>
    <%= paciente.tipo_sangre.nombre_tipo_sangre %>
  </p>

