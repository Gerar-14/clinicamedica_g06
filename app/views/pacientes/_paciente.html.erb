<div id="<%= dom_id paciente %>">
  <p>
    <strong>Nombre paciente:</strong>
    <%= paciente.nombre_paciente %>
  </p>

  <p>
    <strong>Apellido paciente:</strong>
    <%= paciente.apellido_paciente %>
  </p>

  <p>
    <strong>Genero paciente:</strong>
    <%= paciente.genero_paciente %>
  </p>

  <p>
    <strong>Fecha nacimiento:</strong>
    <%= paciente.fecha_nacimiento %>
  </p>

  <p>
    <strong>Direccion:</strong>
    <%= paciente.direccion %>
  </p>

  <p>
    <strong>Telefono:</strong>
    <%= paciente.telefono %>
  </p>

  <p>
    <strong>Telefono celular:</strong>
    <%= paciente.telefono_celular %>
  </p>

  <p>
    <strong>Telefono contacto:</strong>
    <%= paciente.telefono_contacto %>
  </p>

  <p>
    <strong>Correo electronico:</strong>
    <%= paciente.correo_electronico %>
